<%= f.fields_for :line_items, line_item_fields do |builder| %>
  <%= builder.input :product_id, include_blank: true, collection: all_line_items, input_html: { class: 'chosen' } %>
  <%= builder.input :quantity %>
  <li class="input">
    <label class="label">&nbsp;</label>
    <%= link_to "Remove Product", "#", class: "remove_fields" %>
  </li>
  <li></li>
<% end %>
<%= semantic_form_for [:admin, @shipmethod] do |f| %>
  <%= f.semantic_errors :name, :cost %>
  <%= f.inputs "Main Info" do %>
    <%= f.input :name %>
    <%= f.input :cost %>
    <%= f.input :method, :collection => [t(:manual) => "manual", t(:express) => "express"] %>
    <%= f.input :service_phone %>
    <%= f.input :website %>
    <%= f.input :kuaidi_com_code %>
  <% end %>
  <%= f.actions %>
<% end %>
